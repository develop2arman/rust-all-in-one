{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "cargo",
            "command": "clippy",
            "problemMatcher": ["$rustc"],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "label": "rust: cargo clippy"
        },
        {
            "type": "cargo",
            "command": "check",
            "problemMatcher": ["$rustc"],
            "group": {
                "kind": "build",
                "isDefault": false
            },
            "label": "rust: cargo check"
        }
    ]
}
